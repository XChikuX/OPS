{
  "expo": {
    "name": "OPS",
    "slug": "OPS",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/ops/OP.jpg",
    "scheme": "psync",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ops/OP-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ops/OP.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "club.psync.ops"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ops/OPS.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Default",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "904a544b-a9f0-400c-a030-355efc4dab92"
      }
    }
  }
}
