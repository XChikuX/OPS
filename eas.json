{
  "cli": {
    "version": ">=16.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "env": {
        "YARN_VERSION": "4",
        "YARN_ENABLE_INLINE_BUILDS": "true"
      }
    },
    "development": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_NO_FROZEN_LOCKFILE": "1",
        "YARN_VERSION": "4",
        "YARN_ENABLE_INLINE_BUILDS": "true"
      }
    },
    "production": {
      "autoIncrement": false,
      "android": {
        "resourceClass": "medium"
      },
      "ios": {
        "resourceClass": "medium"
      },
      "env": {
        "YARN_VERSION": "4",
        "YARN_ENABLE_INLINE_BUILDS": "true"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "environment": "development"
    }
  }
}