{
  "cli": {
    "version": ">=16.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "distribution": "internal",
      "node": "24.12.0",
      "env": {
        "EXPO_NO_FROZEN_LOCKFILE": "1",
        "YARN_VERSION": "4",
        "YARN_ENABLE_INLINE_BUILDS": "true"
      }
    },
    "production": {
      "autoIncrement": false,
      "android": {
        "resourceClass": "medium"
      },
      "ios": {
        "resourceClass": "medium"
      },
      "node": "24.12.0",
      "env": {
        "YARN_VERSION": "4",
        "YARN_ENABLE_INLINE_BUILDS": "true"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "node": "24.12.0",
      "environment": "development"
    }
  },

  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6466960828"
      }
    }
  }

}
